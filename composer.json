{
    "name": "deadangroup/laravel-whatsapp-widget",
    "description": "A simple package to add a Whatsapp widget to any laravel app",
    "keywords": [
        "deadangroup",
        "laravel",
        "laravel-whatsapp-widget"
    ],
    "homepage": "https://github.com/deadangroup/laravel-whatsapp-widget",
    "license": "MIT",
    "authors": [
        {
            "name": "James Ngugi",
            "email": "james@dgl.co.ke",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^8.37"
    },
    "autoload": {
        "psr-4": {
            "DGL\\LaravelWhatsappWidget\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DGL\\LaravelWhatsappWidget\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "DGL\\LaravelWhatsappWidget\\ModuleServiceProvider"
            ],
            "aliases": {
                "LaravelWhatsappWidget": "DGL\\LaravelWhatsappWidget\\LaravelWhatsappWidgetFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
